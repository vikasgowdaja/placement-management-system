package day12;


@FunctionalInterface
interface A {
void show();
}


class B implements A{
	public void show() {
		System.out.println("in Show");
	}
}

public class Func{
	 public static void main(String[] args) {
		A obj = new B();
//		{
//			public void show() {
//				System.out.println("This Show");
//			}
//		};
		obj.show();
	}
	 
}